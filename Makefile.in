PACKAGE = @PACKAGE@
VERSION = @VERSION@
utildir=@PUBRULE@
appname = @APPNAME@
pubdir = @prefix@
srcdir = @srcdir@

export pubdir utildir appname

TAR = gtar
GZIP_ENV = --best

export targetdir PACKAGE

SUBDIR=Actions Images debian

pages_not_xml = info.xml

include $(utildir)/PubRule

DISTFILES += $(SUBDIR)  \
            RELEASE VERSION   

